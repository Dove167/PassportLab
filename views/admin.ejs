<h1 class="mt-4">Admin Dashboard</h1>
<p class="lead mb-3">Active Sessions</p>

<table class="table">
  <thead>
    <tr>
      <th>User ID</th>
      <th>User Name</th>
      <th>Session Expires</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% sessions.forEach(session => { %>
      <tr>
        <td><%= session.userId %></td>
        <td><%= session.userName %></td>
        <td><%= session.expires %></td>
        <td>
          <form action="/admin/revoke/<%= session.sessionId %>" method="POST">
            <button type="submit" class="btn btn-danger btn-sm" onclick="return confirm('Are you sure you want to revoke this session?')">
                Revoke
              </button>              
          </form>
        </td>
      </tr>
    <% }); %>
  </tbody>
</table>


  
